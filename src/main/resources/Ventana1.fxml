<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Ventana1Controller">
	<center>
		<Button fx:id="bUno" minWidth="100.0" mnemonicParsing="false" onAction="#onButtonAbrirClicked" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" text="Abrir" textFill="RED" BorderPane.alignment="CENTER" />
	</center>
	<opaqueInsets>
		<Insets />
	</opaqueInsets>
	<padding>
		<Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
	</padding>
   <bottom>
      <Label fx:id="lTexto" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <TextField fx:id="tfTexto" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
